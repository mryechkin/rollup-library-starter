{
  "name": "rollup-library-starter",
  "version": "1.0.0",
  "description": "Starter template for building JS libraries using Rollup",
  "main": "./dist/cjs/index.js",
  "module": "./dist/esm/index.js",
  "exports": {
    ".": {
      "import": "./dist/esm/index.js",
      "require": "./dist/cjs/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "sideEffects": false,
  "scripts": {
    "prebuild": "rm -rf dist",
    "build": "rollup -c",
    "lint": "eslint --ignore-path .gitignore --fix src",
    "format": "prettier --write src",
    "prepare": "husky install"
  },
  "keywords": [
    "rollup",
    "javascript",
    "library",
    "esm"
  ],
  "author": "Mykhaylo Ryechkin (https://github.com/mryechkin)",
  "license": "MIT",
  "dependencies": {
    "@babel/runtime": "^7.17.9"
  },
  "devDependencies": {
    "@babel/plugin-transform-runtime": "^7.17.10",
    "@babel/preset-env": "^7.17.10",
    "@babel/preset-react": "^7.16.7",
    "@rollup/plugin-alias": "^3.1.9",
    "@rollup/plugin-babel": "^5.3.1",
    "@rollup/plugin-commonjs": "^22.0.0",
    "@rollup/plugin-image": "^2.1.1",
    "@rollup/plugin-node-resolve": "^13.3.0",
    "eslint": "^8.14.0",
    "eslint-plugin-react": "^7.30.1",
    "husky": "^8.0.1",
    "lint-staged": "^13.0.3",
    "prettier": "^2.6.2",
    "rollup": "^2.73.0",
    "rollup-plugin-analyzer": "^4.0.0",
    "rollup-plugin-copy": "^3.4.0",
    "rollup-plugin-terser": "^7.0.2"
  },
  "peerDependencies": {
    "react": "^16.14.0 || ^17.0.0 || ^18.0.0",
    "react-dom": "^16.14.0 || ^17.0.0 || ^18.0.0"
  },
  "engines": {
    "node": ">=16.0.0 || >=18.0.0"
  },
  "lint-staged": {
    "*.js": "npm run lint",
    "*.{js,css,md*}": "npm run format"
  }
}
