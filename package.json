{
  "name": "rollup-library-starter",
  "version": "2.0.0",
  "description": "Starter template for building JS libraries using Rollup",
  "main": "./dist/cjs/index.js",
  "module": "./dist/esm/index.js",
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/esm/index.js",
      "require": "./dist/cjs/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "sideEffects": false,
  "scripts": {
    "prebuild": "rm -rf dist",
    "build": "rollup -c",
    "lint": "eslint --ignore-path .gitignore --fix src",
    "format": "prettier --write src",
    "prepare": "husky install"
  },
  "keywords": [
    "rollup",
    "javascript",
    "library",
    "esm"
  ],
  "author": "Mykhaylo Ryechkin (https://github.com/mryechkin)",
  "license": "MIT",
  "dependencies": {
    "@babel/runtime": "^7.17.9"
  },
  "devDependencies": {
    "@babel/plugin-transform-runtime": "^7.21.0",
    "@babel/preset-env": "^7.20.2",
    "@babel/preset-react": "^7.18.6",
    "@rollup/plugin-alias": "^4.0.3",
    "@rollup/plugin-babel": "^6.0.3",
    "@rollup/plugin-commonjs": "^24.0.1",
    "@rollup/plugin-image": "^3.0.2",
    "@rollup/plugin-node-resolve": "^15.0.1",
    "eslint": "^8.35.0",
    "eslint-plugin-react": "^7.32.2",
    "husky": "^8.0.3",
    "lint-staged": "^13.1.2",
    "prettier": "^2.8.4",
    "rollup": "^3.18.0",
    "rollup-plugin-analyzer": "^4.0.0",
    "rollup-plugin-copy": "^3.4.0",
    "rollup-plugin-preserve-directives": "^0.1.0",
    "rollup-plugin-terser": "^7.0.2"
  },
  "peerDependencies": {
    "react": "^16.14.0 || ^17.0.0 || ^18.2.0",
    "react-dom": "^16.14.0 || ^17.0.0 || ^18.2.0"
  },
  "engines": {
    "node": ">=16.0.0 || >=18.0.0"
  },
  "lint-staged": {
    "*.js": "npm run lint",
    "*.{js,css,md*}": "npm run format"
  }
}
